{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": ["dom", "esnext"],
		"strict": true,
		"strictFunctionTypes": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"sourceMap": true,
		"esModuleInterop": true
	}
}
